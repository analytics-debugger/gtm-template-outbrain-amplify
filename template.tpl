// David Vallejo (@thyng)
// 2019-08-21
const log = require('logToConsole');
const injectScript = require('injectScript');
const queryPermission = require('queryPermission');
const setInWindow = require('setInWindow');
const callInWindow = require('callInWindow');
const createArgumentsQueue = require('createArgumentsQueue');

// Helpers
// Check if a variable is an array
const isArray = arr => callInWindow('toString.call', arr) === '[object Array]';

// OutBrain Variables
const outbrainScript = 'https://amplify.outbrain.com/cp/obtp.js';
// Assign and Sanitize, Outbrain ID expected as an array
const OB_ADV_ID = (data.advertiserId && isArray(data.advertiserId)) ? data.advertiserId + '' : [data.advertiserId + ''];

// Init the queue
const api = createArgumentsQueue('obApi', 'obApi.queue');

// Set the global variable data
setInWindow('obApi.version', '1.1', true);
setInWindow('obApi.loaded', true, true);
setInWindow('obApi.marketerId', OB_ADV_ID, true);

// Event Call
api('track', data.eventName);
log("GTM-DEBUG :: OUTBRAIN >> Event Fired", data.eventName);

if (queryPermission('inject_script', outbrainScript)) {
  injectScript(outbrainScript, data.gtmOnSuccess, data.gtmOnFailure, outbrainScript);
  log("GTM-DEBUG :: OUTBRAIN >> Loaded", data);
} else {
  log("GTM-DEBUG :: OUTBRAIN >> Current script src is not allowed in permissions configuration");
  data.gtmOnFailure();
}


